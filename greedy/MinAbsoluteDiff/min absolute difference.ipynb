    "n=int(input())\n",
    "list=[]\n",
    "j=input().split()\n",
    "i=0\n",
    "while i<n:\n",
    "    list.append(int(j[i]))\n",
    "    i+=1\n",
    "list.sort()\n",
    "import sys\n",
    "min=sys.maxsize\n",
    "i=0\n",
    "while i<n-1:\n",
    "    diff=abs(list[i]-list[i+1])\n",
    "    if diff<min:\n",
    "        min=diff\n",
    "    i+=1\n",
    "print(min)"
   ]
  }
